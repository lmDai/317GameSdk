<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="NoAnimation" parent="@android:style/Theme.NoTitleBar.Fullscreen">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@style/Animation</item>
    </style>

    <style name="Animation">
        <item name="android:activityOpenEnterAnimation">@null</item>
        <item name="android:activityOpenExitAnimation">@null</item>
        <item name="android:activityCloseEnterAnimation">@null</item>
        <item name="android:activityCloseExitAnimation">@null</item>
        <item name="android:taskOpenEnterAnimation">@null</item>
        <item name="android:taskOpenExitAnimation">@null</item>
        <item name="android:taskCloseEnterAnimation">@null</item>
        <item name="android:taskCloseExitAnimation">@null</item>
        <item name="android:taskToFrontEnterAnimation">@null</item>
        <item name="android:taskToFrontExitAnimation">@null</item>
        <item name="android:taskToBackEnterAnimation">@null</item>
        <item name="android:taskToBackExitAnimation">@null</item>
    </style>

    <style name="AppBaseTheme" parent="@android:style/Theme.Light" />

    <style name="AppTheme" parent="@style/AppBaseTheme" />

    <style name="wygamesdk_loading" parent="@android:style/Widget.ProgressBar.Large">
        <item name="android:indeterminateDrawable">@anim/wygamesdk_loading</item>
    </style>

    <style name="wygamesdk_customDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowCloseOnTouchOutside">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="wygamesdk_main_background">@drawable/wygamesdk_bg</item>
        <item name="wygamesdk_img_user">@drawable/wygamesdk_ic_username</item>
        <item name="wygamesdk_img_phone">@drawable/wygamesdk_ic_phone</item>
        <item name="wygamesdk_img_password">@drawable/wygamesdk_ic_password</item>
        <item name="wygamesdk_confirm_button_style">@style/wygamesdk_btn_confirm</item>
        <item name="wygamesdk_close_button_style">@style/wygamesdk_btn_close</item>
        <item name="wygamesdk_back_button_style">@style/wygamesdk_btn_back</item>
        <item name="wygamesdk_title_text_style">@style/wygamesdk_textView_title</item>
        <item name="wygamesdk_title_logo_style">@style/wygamesdk_imageView_title</item>
        <item name="wygamesdk_basic_text_style">@style/wygamesdk_textView_basic</item>
        <item name="wygamesdk_info_text_style">@style/wygamesdk_textView_info</item>
        <item name="wygamesdk_info_name_style">@style/wygamesdk_textView_name</item>
        <item name="wygamesdk_text_image_style">@style/wygamesdk_textView_arraw</item>
        <item name="wygamesdk_text_no_image_style">@style/wygamesdk_textView_no_arraw</item>
        <item name="wygamesdk_basic_checkbox_style">@style/wygamesdk_checkbox</item>
        <item name="wygamesdk_basic_editText_style">@style/wygamesdk_editText_basic</item>
        <item name="wygamesdk_switch_img_show">@drawable/wygamesdk_password_show</item>
        <item name="wygamesdk_switch_img_hide">@drawable/wygamesdk_password_hide</item>
        <item name="wygamesdk_item_user_text_style">@style/wygamesdk_item_user_textView</item>
    </style>

    <style name="wygamesdk_customPaymentTranslucentTheme" parent="@android:style/Theme.Translucent.NoTitleBar.Fullscreen">
        <item name="wygamesdk_close_button_style">@style/wygamesdk_btn_close</item>
        <item name="wygamesdk_back_button_style">@style/wygamesdk_btn_back</item>
        <item name="wygamesdk_title_text_style">@style/wygamesdk_textView_title</item>
        <item name="wygamesdk_title_logo_style">@style/wygamesdk_imageView_title</item>
    </style>

    <style name="wygamesdk_btn_confirm">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/wygamesdk_btn_confirm_selector</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="wygamesdk_btn">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="wygamesdk_btn_close">
        <item name="android:background">@drawable/wygamesdk_ic_close</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">15dp</item>
    </style>

    <style name="wygamesdk_btn_back">
        <item name="android:background">@drawable/wygamesdk_ic_back</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">15dp</item>
    </style>

    <style name="wygamesdk_textView_title">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/wygamesdk_text_title_color</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="wygamesdk_textView_basic">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/wygamesdk_text_color</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="wygamesdk_textView_info">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/wygamesdk_text_info_color</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="wygamesdk_textView_name">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/wygamesdk_text_name_color</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="wygamesdk_textView_arraw">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/wygamesdk_text_title_color</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="wygamesdk_textView_no_arraw">
        <item name="android:textColor">@color/wygamesdk_text_title_color</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="wygamesdk_imageView_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/wygamesdk_name_logo</item>
    </style>

    <style name="wygamesdk_editText_basic" parent="@android:style/Widget.EditText">
        <item name="android:textSize">@dimen/wygamesdk_input_text_size</item>
        <item name="android:textColor">@color/wygamesdk_edit_color</item>
        <item name="android:ellipsize">end</item>
        <item name="android:background">@null</item>
        <item name="android:gravity">center|left</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:paddingLeft">8.0dip</item>
        <item name="android:paddingTop">8.0dip</item>
        <item name="android:paddingBottom">8.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:ems">8</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="wygamesdk_item_user_textView" parent="@android:style/Widget.TextView">
        <item name="android:textSize">@dimen/wygamesdk_input_text_size</item>
        <item name="android:textColor">@color/wygamesdk_text_info_color</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center|left</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:paddingLeft">8.0dip</item>
        <item name="android:paddingTop">8.0dip</item>
        <item name="android:paddingBottom">8.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:ems">8</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="wygamesdk_checkbox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@color/wygamesdk_text_agree_color</item>
        <item name="android:paddingLeft">4.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="noAnimationm">
        <item name="android:activityOpenEnterAnimation">@null</item>
        <item name="android:activityOpenExitAnimation">@null</item>
        <item name="android:activityCloseEnterAnimation">@null</item>
        <item name="android:activityCloseExitAnimation">@null</item>
        <item name="android:taskOpenEnterAnimation">@null</item>
        <item name="android:taskOpenExitAnimation">@null</item>
        <item name="android:taskCloseEnterAnimation">@null</item>
        <item name="android:taskCloseExitAnimation">@null</item>
        <item name="android:taskToFrontEnterAnimation">@null</item>
        <item name="android:taskToFrontExitAnimation">@null</item>
        <item name="android:taskToBackEnterAnimation">@null</item>
        <item name="android:taskToBackExitAnimation">@null</item>
    </style>

    <style name="translucentm" parent="@android:style/Theme.Translucent.NoTitleBar.Fullscreen">
        <item name="android:windowBackground">@color/wygamesdk_color_trumpet</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/noAnimationm</item>
    </style>

    <style name="translucent" parent="@android:style/Theme.Translucent.NoTitleBar.Fullscreen">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/noAnimationm</item>
    </style>

    <style name="MultipleStatusView" />

    <style name="MultipleStatusView.Content">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_margin">8dp</item>
        <item name="android:textColor">#a9b7b7</item>
        <item name="android:textSize">16sp</item>
    </style>
</resources>